#include <stdio.h>
#define MAXSIZE 100

int main()
{
    int arr[MAXSIZE] ,i , size ,elment , postion, num ;
	printf("enter size of array ");
	scanf("%d" , &size);
	printf("Input array elements: ");
	for(i = 0 ; i<size ;i++){
		scanf("%d", &arr[i]);
	}
	
	printf("Array elements after insertion : ");
	for(i=0 ;i<size ; i++)
	{
		printf("%d\n", arr[i]);
	}


}

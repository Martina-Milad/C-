#include <stdio.h>
#define MAXSIZE 100

int main()
{
    int arr[MAXSIZE] ,i , size ,elment , postion, num ;
	printf("enter size of array ");
	scanf("%d" , &size);
	printf("Input array elements: ");
	for(i = 0 ; i<size ;i++){
		scanf("%d", &arr[i]);
	}
	
	int count = 0;
	printf("sum = ");
	for(i=0 ;i<size ; i++)
	{
		count = count +arr[i];
	}
		printf("%d" , count);



}
